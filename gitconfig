[user]
	name = William Falk-Wallace
	email = wgf2104@columbia.edu
[core]
	excludesfile = ~/.gitignore_global
[alias]
	a = add
	s = status
	c = commit
	cl = clone
	f = fetch
	cm = commit -m
	rem = remote
	br = branch
	co = checkout
	df = diff
	lg = log -p
	p = push
	pu = pull
	url = "!f() { sha=$(git rev-parse ${1:-HEAD}); hub browse -u -- commit/$sha; }; f"
	pb = "!f() { sha=$(git rev-parse ${1:-HEAD}); hub browse -u -- commit/$sha; }; f | pbcopy"
	commits = !hub browse -- commits
	issues = !hub browse -- issues
	wiki = !hub browse -- wiki
	settings = !hub browse -- settings
	unstage = reset HEAD --
	last = log -1 HEAD
	vis = !gitk
	ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	r = reset
	r1 = reset HEAD^
	r2 = reset HEAD^^
	rh = reset --hard
	rh1 = reset HEAD^ --hard
	rh2 = reset HEAD^^ --hard
	sl = stash list
	sp = stash pop
	st = stash
[credential]
	helper = osxkeychain
[push]
	default = current
[pull]
	rebase = true
[color]
    diff = auto
    status = auto
    branch = auto
    grep = auto
    interactive = auto
    ui = auto
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    whitespace = red reverse
    frag = cyan
    old = red bold
    new = green bold
[color "status"]
    added = green
    changed = yellow
    untracked = red
